package com.r2cf.entity;import java.util.Date;import javax.persistence.Table;/******************************************************************************* * 日志记录表（t_logs_info） * T_LOGS_INFO --> TLogsInfo  * <table explanation> * @author 2017-02-12 16:44:39 *  */	@Table(name="t_logs_info")public class LogsInfoBean implements java.io.Serializable {	//field	/**  **/	private Long id;	/** 操作人 **/	private Long operator;	/** 操作内容 **/	private String operatorContent;	/** 操作时间 **/	private Date operatorTime;	/** 备注 **/	private String remark;	//method	public Long getId() {		return id;	}	public void setId(Long id) {		this.id = id;	}	public Long getOperator() {		return operator;	}	public void setOperator(Long operator) {		this.operator = operator;	}	public String getOperatorContent() {		return operatorContent;	}	public void setOperatorContent(String operatorContent) {		this.operatorContent = operatorContent;	}	public Date getOperatorTime() {		return operatorTime;	}	public void setOperatorTime(Date operatorTime) {		this.operatorTime = operatorTime;	}	public String getRemark() {		return remark;	}	public void setRemark(String remark) {		this.remark = remark;	}	//override toString Method 	public String toString() {		StringBuffer sb=new StringBuffer();		sb.append("{");		sb.append("'id':'"+this.getId()+"',");		sb.append("'operator':'"+this.getOperator()+"',");		sb.append("'operatorContent':'"+this.getOperatorContent()+"',");		sb.append("'operatorTime':'"+this.getOperatorTime()+"',");		sb.append("'remark':'"+this.getRemark()+"',");		sb.append("}");		return sb.toString();	}	//return String[] filed; 	public String[] getField() {		return new String[]{"id","operator","operatorContent","operatorTime","remark"};	}}